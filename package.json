{
  "name": "support-chat",
  "version": "1.0.0",
  "description": "REST service for vulnerability analysis using OpenAI API",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "empty-store": "node file-search/empty-store.js",
    "fetch:portal": "node fetch-portal.js",
    "fetch:manual": "node fetch-manual.js",
    "fetch": "npm run empty-store && npm run fetch:portal && npm run fetch:manual",
    "lint": "eslint . --ext .js,.mjs,.svelte",
    "format": "prettier --write \"**/*.{js,mjs,svelte}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "happy-dom": "^17.4.4",
    "marked": "15.0.12",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.4"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  }
}
